{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "Node",
    "jsx": "preserve",
    "baseUrl": "./",
    "isolatedModules": true,
    "types": ["vite/client"],
    "paths": {
      "@/*": ["src/*"]
    }

  },
  "include": ["src/**/*.ts", "src/**/*.vue", "src/**/*.ts"],
  "exclude": ["node_modules"]
}